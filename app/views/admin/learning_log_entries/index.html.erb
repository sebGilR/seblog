<p id="notice"><%= notice %></p>

<h1>Learning Log Updates</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th>Learning log</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @learning_log_entries.each do |learning_log_entry| %>
      <tr>
        <td><%= learning_log_entry.description %></td>
        <td><%= learning_log_entry.learning_log_id %></td>
        <td><%= link_to 'Show', learning_log_entry %></td>
        <td><%= link_to 'Edit', edit_learning_log_entry_path(learning_log_entry) %></td>
        <td><%= link_to 'Destroy', learning_log_entry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Learning Log Update', new_learning_log_entry_path %>
